{
  "name": "route-descriptor",
  "version": "1.0.0",
  "description": "Single source of truth for routing in your application",
  "repository": "https://github.com/Tyrrrz/route-descriptor",
  "author": "Alexey 'Tyrrrz' Golub",
  "license": "MIT",
  "keywords": [
    "route",
    "routing"
  ],
  "module": "./build/esm/index.js",
  "main": "./build/cjs/index.js",
  "types": "./build/esm/index.d.ts",
  "files": [
    "./build"
  ],
  "dependencies": {
    "path-to-regexp": "^6.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.3",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "build": "rimraf ./build && tsc -p tsconfig.build.esm.json && tsc -p tsconfig.build.cjs.json",
    "test": "jest",
    "lint": "eslint */**/*.{js,jsx,ts,tsx} --fix",
    "prepare": "npm run build"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
